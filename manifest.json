{
    "manifest_version": 2,
    "name": "Instaling Bot",
    "version": "0.4.4.1",
    "content_scripts": [
        {
            "matches": [
                "https://instaling.pl/ling2/html_app/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "img/logo.png",
        "default_popup": "popup/popup.html",
        "default_title": "He he"
    },
    "author": "Endiasz"
}