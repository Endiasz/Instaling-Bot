{
    "manifest_version": 3,
    "name": "Instaling Bot",
    "version": "0.51",
    "content_scripts": [
        {
            "matches": [
                "https://instaling.pl/ling2/html_app/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "permissions": [
        "tabs"
    ],
        "background": {
       "service_worker": "background.js"
    },
    "action": {
        "default_icon": "img/logo.png",
        "default_popup": "popup/popup.html",
        "default_title": "Instalig bot"
    },
    "author": "Endiasz"
}
